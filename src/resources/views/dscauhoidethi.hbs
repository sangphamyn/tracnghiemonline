<div class="container">
  <table class="table list-teacher mt-4">
    <thead>
      <tr>
        <th scope="col">STT</th>
        <th scope="col">Câu hỏi</th>
        <th scope="col">Chủ đề</th>
        <th scope="col">Mức độ</th>
      </tr>
    </thead>
    <tbody>
      {{#each dscauhoi}}
      <tr>
        <th scope="row">{{sum @index 1}}</th>
        <td>{{this.CH_noidung}}</td>
        <td>{{this.CD_name}}</td>
        <td>{{this.DK_name}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>